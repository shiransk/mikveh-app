<%= form_tag '/mikveh', method: :post do %>

<div>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
</div>


<div>
  <%= label_tag :location %>
  <%= text_field_tag :location %>
</div>


<div>
  <%= label_tag :rooms %>
  <%= text_field_tag :rooms %>
</div>


<div>
  <%= hidden_field_tag :user_id, current_user.id %>
</div>



 <%= submit_tag "Submit" %>

<% end %>